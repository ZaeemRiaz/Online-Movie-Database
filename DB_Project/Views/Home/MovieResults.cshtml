@using DB_Project.Models
@model List<Movie>
@{
    int i = 1;
}

<html>

<head>
    <meta charset="utf-8" />
    <title>Movie Results</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
</head>

<body>
    @foreach (Movie m in Model)
    {
        <h2><a href="../../Home/MovieDetails?movieID=@m.movieID"> @m.title</a></h2>
        <img src="@m.picture" alt="Movie names" height="100" width="100"/>
        i = i + 1;
    }
</body>

</html>